{
  "name": "harbour-ts-sdk",
  "version": "1.0.0",
  "main": "index.ts",
  "repository": "git@github.com:harbour-tech/harbour-ts-sdk.git",
  "author": "Harbour",
  "license": "MIT",
  "private": true,
  "dependencies": {
    "@bufbuild/protobuf": "^1.6.0",
    "@connectrpc/connect": "^1.3.0",
    "@connectrpc/connect-web": "^1.3.0"
  },
  "devDependencies": {
    "@connectrpc/protoc-gen-connect-es": "^1.3.0",
    "@mysten/sui.js": "^0.48.1",
    "eslint-plugin-prettier": "^5.1.0",
    "ethers": "^6.9.1",
    "prettier": "^3.1.1"
  },
  "scripts": {
    "buf:generate": "buf generate && sed -i '' s/_pb.js/_pb/ src/gen/*/v1/public_*.ts",
    "buf:sync": "rm -rf proto && rm -rf _tmp && mkdir -p proto/ramp  && git clone --depth 1 git@github.com:harbour-tech/be-ramp-service.git _tmp && mv _tmp/proto/ramp proto && rm -rf _tmp",
    "buf": "yarn buf:sync && yarn buf:generate && yarn prettier --write ./src/gen/"
  }
}
